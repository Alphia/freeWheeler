package com.trailblazers.freewheelers.model;

public class Country {

    public static final long UK_ID = 1;
    public static final long USA_ID = 2;
    public static final long FRANCE_ID = 3;
    public static final long GERMANY_ID = 4;
    public static final long CANADA_ID = 5;
    public static final long ITALY_ID = 6;

    private String country_name;
    private long country_id;

    public Country() {
        this.country_id = 0L;
    }

    public Country(String name) {
        this.country_id = 0L;
        this.country_name=name;
    }

    public String getCountry_name() {
        return country_name;
    }

    public void setCountry_name(String country_name) {
        this.country_name = country_name;
    }

    public long getCountry_id() {
        return country_id;
    }

    public void setCountry_id(long country_id) {
        this.country_id = country_id;
    }
}
